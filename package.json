{
  "name": "jblog",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build && npm run clean && npm run copyIndex && npm run copyStatic",
    "lint": "vue-cli-service lint",
    "clean": "rimraf ../main/resources/static/*",
    "copyIndex": "cpx ./dist/index.html ../main/resources/templates",
    "copyStatic": "cpx ./dist/favicon.ico ../main/resources/static && cpx ./dist/{css,fonts,img,js}/** ../main/resources/static"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "core-js": "^2.6.5",
    "element-ui": "^2.4.5",
    "moment": "^2.24.0",
    "normalize.css": "^8.0.1",
    "vue": "^2.6.10",
    "vue-router": "^3.0.3",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.8.0",
    "@vue/cli-plugin-eslint": "^3.8.0",
    "@vue/cli-service": "^3.8.0",
    "babel-eslint": "^10.0.1",
    "cpx": "^1.5.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "node-sass": "^4.9.2",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.0.3",
    "vue-cli-plugin-element": "^1.0.1",
    "vue-template-compiler": "^2.6.10"
  }
}